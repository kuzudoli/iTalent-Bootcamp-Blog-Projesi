@model PostsWithPageCount

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Category</th>
            <th scope="col">LikeCount</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var post in Model.Posts){
            <tr>
                <td>@post.Id</td>
                <td>@post.Title</td>
                <td>@post.Category.Name</td>
                <td>@post.LikeCount</td>
            </tr>
        }
    </tbody>
</table>